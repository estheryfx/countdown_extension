{
  "manifest_version": 2,
  "name": "Why Wait",
  "version": "0.0.1",
  "description": "Improve productivity and tackle procrastination for person(like myself)",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png",
    "default_title": ""
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "tabs",
    "activeTab"
  ]
}